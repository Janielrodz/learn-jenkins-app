{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "LearnJenkinsApp-TaskDefinition-Prod-ARM",
    "containerDefinitions": [
        {
            "name": "learnjenkinsapp",
            "image": "117820557163.dkr.ecr.us-east-2.amazonaws.com/learnjenkinsapp:1.0.114",
            "portMappings": [
                {
                    "name": "nginx-80-tip",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::117820557163:role/ecsTaskExecutionRole",
    "runtimePlatform": {
    "cpuArchitecture": "ARM64",
    "operatingSystemFamily": "LINUX"
}
}